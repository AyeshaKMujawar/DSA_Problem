#include <iostream>
using namespace std;

class Human{
    public:

    int age;
    int weight;

    void sleep()
        {
            cout<<"he is sleeping"<<endl;
            
        }

        void eat(){
         cout<<"He is eating"<<endl;


        }
};
    


int main()
{
    Human chitti;
    chitti.age=21;
    chitti.weight=40;
    chitti.sleep();
    chitti.eat();
    
}
